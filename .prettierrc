// .prettierrc
{
    "overrides": [{
        "files": ".prettierrc",
        "options": { 
            "parser": "json" 
        }
    }],
    "useTabs": false,
    "printWidth": 80,
    "tabWidth": 2,
    "singleQuote": false,
    "trailingComma": "none",
    "jsxBracketSameLine": false,
    "parser": "babylon",
    "noSemi": true,
    "rcVerbose": true
}
